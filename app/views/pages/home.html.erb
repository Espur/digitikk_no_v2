                <%= form_for @contact, url: contact_pages_path do |f| %>
                    <div class="form-group">
                        <%= f.label :navn %>
                        <%= f.text_field :navn, class: 'form-control', placeholder: 'Ditt fulle navn' %>
                    </div>
                    
                    <div class="form-group">
                        <%= f.label :epost %>
                        <%= f.email_field :epost, class: 'form-control', placeholder: 'Din epost adresse' %>
                    </div>
                    
                    <div class="form-group">
                        <%= f.label :comments %>
                        <%= f.text_area :comments, class: 'form-control', placeholder: 'Hva kan jeg hjelpe deg med?' %>
