                    <li><i class="fa fa-file-code-o fa-fw" aria-hidden="true"></i>Web Designer</li>

                    <li><i class="fa fa-video-camera fa-fw" aria-hidden="true"></i>Video Editor</li>

                    <li><i class="fa fa-terminal fa-fw" aria-hidden="true"></i>Back-end utvikler</li>

                </ul>
                <ul class="values-right pull-right wow fadeInUp">
                    <li><i class="fa fa-youtube-play fa-fw" aria-hidden="true"></i>Youtuber</li>

                    <li><i class="fa fa-trademark fa-fw" aria-hidden="true"></i>Branding</li>

                    <li><i class="fa fa-pencil fa-fw" aria-hidden="true"></i>Grafisk Design</li>

                </ul>
            </div>
            <div class="col-md-6 col-right text-right wow fadeInRight">
                <%= image_tag("esp_png.png") %>
            </div>
        </div>
    </div>
</section>

